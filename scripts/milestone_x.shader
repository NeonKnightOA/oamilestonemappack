//portal-oamcmp



textures/milestone_x/blue_oamcmp_edge
{
	surfaceparm trans
	surfaceparm nonsolid
	surfaceparm nomarks
	cull none
	
	{
		map $lightmap
		blendFunc GL_DST_COLOR GL_ONE
		depthFunc equal
	}
	{
		map textures/milestone_x/blue_oamcmp_edge.tga
		blendFunc GL_ONE GL_ONE
		tcMod turb 3 .3 3 .3
		//tcMod stretch square -1 1 2.5 0.8
		tcMod rotate 45
	}

	{
		map textures/milestone_x/oamcmp_screen_green_noise2.tga
		blendfunc add
	        rgbGen identity
		tcmod scroll 2 2
		
	}
}

