//adver_screen


textures/milestone_x/oamcmp_screen_green
{
	q3map_lightimage textures/milestone_x/oamcmp_screen_green.tga
        q3map_surfacelight 100


	{
		map textures/milestone_x/oamcmp_screen_green.tga
	        rgbGen wave square 0 1 0 .35
	}

	{
		map textures/milestone_x/oamcmp_screen_green_noise.tga
		blendfunc add
	        rgbGen identity
		tcmod scroll 2 2
		
	}

	{
		map textures/milestone_x/oamcmp_screen_green_lines.tga
		blendfunc add
	        rgbGen identity
		tcmod scroll 3 3
	}


	{
		map $lightmap
	        rgbGen identity
		blendfunc gl_dst_color gl_zero
	}

	{
		map $lightmap
		tcgen environment
		tcmod scale .5 .5
	        rgbGen wave sin .15 0 0 0
		blendfunc add
	}	          		
}   


