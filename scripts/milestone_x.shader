// OAMCMP Flaggs Store //



textures/milestone_x/OACMP_flagg001_blue
{
	
	surfaceparm nomarks
	surfaceparm nonsolid
	nopicmip
	cull none
	deformVertexes wave 90 sin 0 2 0 0.2

	{
		map textures/milestone_x/OACMP_flagg001_blue.tga
                alphaFunc GE128
		depthWrite
		rgbGen vertex
        }
        {
		map $lightmap
		rgbGen identity
		blendFunc filter
		depthFunc equal
	}


}


