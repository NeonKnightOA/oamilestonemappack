//Spawnpoint

textures/milestone_x/oamcmp_spawnpoint_circle
{
	surfaceparm nomarks
	surfaceparm trans
	surfaceparm pointlight
	nopicmip
	polygonoffset
	{
		clampmap textures/milestone_x/oamcmp_spawnpoint_circle.tga
		blendfunc add
		rgbGen Vertex
		tcMod rotate 45
	}
	{
		map textures/milestone_x/oamcmp_spawnpoint_skull.tga
		blendfunc add
		rgbGen Vertex
	}
}


